{% extends 'base.html.twig' %}

 {% block body %}
    <div>
        {% if messages|length > 0 %}
            {% for key, message in messages %}
            <div>
            <a href="{{ path('app_show_one', {id: key}) }}">{{message.message}}</a>
            </div>
                
                    {{include('hello/_message.html.twig', {message: message})}}

            {% endfor %}
        {% else %}
            There is nothing yet!
        {% endif %}
    </div>
 {% endblock %}
